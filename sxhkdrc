#keybindings
#Volume
XF86AudioRaiseVolume
	pamixer -i 2

XF86AudioLowerVolume
	pamixer -d 2

XF86AudioMute
	pamixer -t

#Brightness 
XF86MonBrightnessUp
	brillo -q -A 2

XF86MonBrightnessDown
	brillo -q -U 2
	
#Screenshot
Print 
       maim ~/pupa/screenshots/full_`date '+%S_%M_%H'`.png

shift + Print 
       maim -s ~/pupa/screenshots/sel_`date '+%S_%M_%H'`.png

alt + x
       maim -s | xclip -selection clipboard -t image/png 

#run launcher
super + p 
       rofi -show drun
super + shift + e 
       rofi -show emoji
super + shift + m
       rofi -show calc -no-show-match -no-sort 

#relax2020
alt + p 
      ~/git/relax/pause.sh &

